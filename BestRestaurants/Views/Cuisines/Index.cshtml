@{
  Layout = "_Layout";
}

@using BestRestaurants.Models;

<h1>Cuisines</h1>

@if (@Model.Count == 0)
{
  <h3>No Cuisines have been added yet!</h3>
} 

<ul>
  @foreach(Cuisines Cuisines in Model)
  {
    <li>@Html.ActionLink($"{Cuisines.Name}", "Details", new { id = Cuisines.CuisinesId })</li>
  }
</ul>

<p>@Html.ActionLink("Add new Cuisines", "Create")</p>
<p>@Html.ActionLink("Home", "Index", "Home")</p>